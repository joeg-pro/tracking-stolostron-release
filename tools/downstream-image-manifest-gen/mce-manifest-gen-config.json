        "image-namespace": "multicluster-engine",

        "image-list": [
            {"acm-component-name": "backplane_must_gather",            "build-info-env-var": "MUST_GATHER"},
            {"acm-component-name": "multicloud_manager",               "build-info-env-var": "MULTICLOUD_MANAGER"},
            {"acm-component-name": "managedcluster_import_controller", "build-info-env-var": "MANAGEDCLUSTER_IMPORT_CONTROLLER"},
            {"acm-component-name": "openshift_hive",                   "build-info-env-var": "OPENSHIFT_HIVE_OPERATOR"},
            {"acm-component-name": "placement",                        "build-info-env-var": "PLACEMENT"},
            {"acm-component-name": "registration",                     "build-info-env-var": "REGISTRATION"},
            {"acm-component-name": "registration_operator",            "build-info-env-var": "REGISTRATION_OPERATOR"},
            {"acm-component-name": "work",                             "build-info-env-var": "WORK"},
            {"acm-component-name": "backplane_operator",               "build-info-env-var": "MULTICLUSTER_ENGINE_OPERATOR"}
